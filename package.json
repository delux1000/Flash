  },
  "homepage": "https://github.com/yourusername/eth-rpc-simulator#readme",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/tests/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "routes/",
      "controllers/",
      "models/",
      "middleware/",
      "utils/"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "coverage/",
      "tests/",
      "*.test.js"
    ],
    "delay": "1000",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
